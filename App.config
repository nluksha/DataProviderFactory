<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<add key="provider" value="System.Data.SqlClient" />
	</appSettings>
	<connectionStrings>
		<add name="AutoLogSqlProvider" connectionString="Data Source=.; Initial Catalog=AutoLog; Integrated Security=True"/>
		<add name="AutoLogOleDbProvider" connectionString="Provider=SQLNCLI11; Data Source=.; Initial Catalog=AutoLog; Integrated Security=SSP"/>
	</connectionStrings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
</configuration>